{% extends "layout.njk" %}



    {% block content %}

    <div id="wrapper">
        <img src="{{about.avatar_url}}" alt="Leonardo Gottardi">
        <h1>{{about.name}}</h1>
        <h2>{{about.role}}</h2>
    </div>

    <div id="links-footer" class="links">
    {% for link in about.links %}
        <a href="{{link.url}}" target="_blank">{{link.name}}</a>
    {% endfor %}
    </div>
    {% endblock %}
